{
  "name": "lib.reviews",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "grunt",
    "start-dev": "pm2 start process.json --only app-development --no-daemon",
    "del-dev": "pm2 delete app-development",
    "restart-dev": "pm2 restart app-development",
    "start-prod": "pm2 start process.json --only app-production",
    "del-prod": "pm2 delete app-production",
    "restart-prod": "pm2 restart app-production",
    "test": "NODE_ENV=development node node_modules/ava/cli.js --verbose tests/**",
    "test-debug": "NODE_ENV=development DEBUG=libreviews:* node node_modules/ava/cli.js --verbose tests/**",
    "test-watch": "NODE_ENV=development DEBUG=libreviews:tests node node_modules/ava/cli.js --watch --verbose tests/**",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "make-docs": "node_modules/jsdoc/jsdoc.js -c jsdocConf.json -r frontend -d static/devdocs/frontend && node_modules/jsdoc/jsdoc.js -c jsdocConf.json -r models/ util/ routes/ app.js -d static/devdocs/backend"
  },
  "pre-commit": [
    "test"
  ],
